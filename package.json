    "fastify-static": "^4.5.0",
    "frappe-gantt": "^0.5.0",
    "immutable-set": "^2.2.1",
    "is-docker": "^2.0.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.34",
    "node-sass": "^6.0.1",
